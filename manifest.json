{
    "manifest_version": 2,
    "name": "Youtube Endscreen Content Blocker",
    "version": "1.0",
    "description": "Blocks Youtube elements",
    "permissions": [
        "*://*/*"
    ],
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": [ "js/content.js" ]
        },
        {
            "matches": ["*://*/*"],
          "js": [ "js/iframe-detector.js" ]
        }
    ]
}